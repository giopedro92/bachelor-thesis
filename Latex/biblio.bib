@book{cottingham_introduction_2007,
    title     = {An Introduction to the Standard Model of Particle Physics},
    author    = {W. N. Cottingham and D. A. Greenwood},
    year      = {2007},
    publisher = {Cambridge University Press},
    address   = {Cambridge},
    edition   = {2nd},
    doi       = {10.1017/9781009401685},
    isbn      = {978-0521852494},
    url       = {https://library.oapen.org/handle/20.500.12657/64010},
    abstract  = {The second edition of this introductory graduate textbook provides a concise yet accessible introduction to the Standard Model. It has been updated to account for the successes of the theory of strong interactions and the observations on matter–antimatter asymmetry. It gives a coherent presentation of the phenomena and theory that describe neutrino mass as well as an account of progress in the theory of strong interactions. The book develops clearly the theoretical concepts from the electromagnetic and weak interactions of leptons and quarks to the strong interactions of quarks. Each chapter ends with problems, with hints to selected problems provided at the end of the book. The mathematical treatments are suitable for graduates in physics, while more sophisticated mathematical ideas are developed in the text and appendices.}
}

